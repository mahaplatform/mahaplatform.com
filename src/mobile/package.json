{
  "name": "maha-cordova",
  "description": "Mobile First Application Platform",
  "version": "0.1.0",
  "main": "index.js",
  "scripts": {
    "ios:simulator": "cordova run ios --buildFlag='-UseModernBuildSystem=0' --target=iPhone-8",
    "ios:device": "cordova run ios --buildFlag='-UseModernBuildSystem=0'",
    "android": "cordova run android --release --device"
  },
  "author": "Apache Cordova Team",
  "license": "Apache-2.0",
  "dependencies": {
    "cordova-android": "^8.0.0",
    "cordova-ios": "^5.0.1",
    "cordova-plugin-camera": "^4.0.3",
    "cordova-plugin-customurlscheme": "^4.4.0",
    "cordova-plugin-contacts": "^3.0.1",
    "cordova-plugin-device": "^2.0.2",
    "cordova-plugin-firebase": "^2.0.5",
    "cordova-plugin-geolocation": "^4.0.1",
    "cordova-plugin-insomnia": "https://github.com/EddyVerbruggen/Insomnia-PhoneGap-Plugin.git",
    "cordova-plugin-keyboard": "^1.2.0",
    "cordova-plugin-safariviewcontroller": "^1.6.0",
    "cordova-plugin-screen-orientation": "^3.0.1",
    "cordova-plugin-statusbar": "^2.4.2",
    "cordova-plugin-vibration": "^3.1.0",
    "cordova-plugin-whitelist": "^1.3.3"
  },
  "cordova": {
    "platforms": [
      "ios",
      "android"
    ],
    "plugins": {
      "cordova-plugin-camera": {},
      "cordova-plugin-customurlscheme": {
        "URL_SCHEME": "maha"
      },
      "cordova-plugin-contacts": {},
      "cordova-plugin-device": {},
      "cordova-plugin-firebase": {},
      "cordova-plugin-geolocation": {},
      "cordova-plugin-insomnia": {},
      "cordova-plugin-keyboard": {},
      "cordova-plugin-safariviewcontroller": {},
      "cordova-plugin-screen-orientation": {},
      "cordova-plugin-statusbar": {},
      "cordova-plugin-vibration": {},
      "cordova-plugin-whitelist": {},
      "cordova-plugin-app-version": {}
    }
  },
  "devDependencies": {
    "cordova-plugin-app-version": "^0.1.9"
  }
}