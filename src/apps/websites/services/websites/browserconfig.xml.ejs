<?xml version="1.0" encoding="utf-8"?>
<browserconfig>
  <msapplication>
    <tile>
      <square70x70logo src="<%- icon(site.favicon, 'webp', { dpi: 1, fit: 'cover', w: 70, h: 70 }) %>"/>
      <square150x150logo src="<%- icon(site.favicon, 'webp', { dpi: 1, fit: 'cover', w: 150, h: 150 }) %>"/>
      <square310x310logo src="<%- icon(site.favicon, 'webp', { dpi: 1, fit: 'cover', w: 310, h: 310 }) %>"/>
      <wide310x150logo src="<%- icon(site.favicon, 'webp', { dpi: 1, fit: 'cover', w: 310, h: 150 }) %>"/>
      <TileColor><%- site.background_color %></TileColor>
    </tile>
  </msapplication>
</browserconfig>
